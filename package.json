{
  "name": "ateam-bn-backend",
  "version": "1.0.0",
  "description": "We make company global travel and accommodation easy and convenient for the strong workforce of savvy members of staff, by leveraging the modern web.",
  "main": "index.js",
  "scripts": {
    "test": "npm run droptables:test && npm run createtables:test && npm run seed:test && cross-env NODE_ENV=test nyc --reporter=text --reporter=lcov mocha --require @babel/register --require babel-polyfill ./server/tests/*.js --timeout 100000 --exit",
    "server": "nodemon --exec babel-node ./server/index.js",
    "start": "cross-env NODE_ENV=production npx sequelize-cli db:migrate && cross-env NODE_ENV=production babel-node ./server/index.js",
    "createtables:dev": "npx sequelize db:migrate",
    "seed:dev": "npx sequelize db:seed:all",
    "droptables:dev": "npx sequelize db:migrate:undo:all",
    "createtables:test": "cross-env NODE_ENV=test npx sequelize db:migrate",
    "seed:test": "cross-env NODE_ENV=test npx sequelize db:seed:all",
    "coveralls": "npm test && nyc report --reporter=text-lcov | coveralls",
    "droptables:test": "cross-env NODE_ENV=test npx sequelize db:migrate:undo:all",
    "kill": "Taskkill /IM node.exe /F",
    "lint": "eslint ./server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Stackup-Rwanda/ateam-bn-backend.git"
  },
  "author": "The A Team",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Stackup-Rwanda/ateam-bn-backend/issues"
  },
  "homepage": "https://github.com/Stackup-Rwanda/ateam-bn-backend#readme",

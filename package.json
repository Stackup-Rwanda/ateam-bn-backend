    "@babel/runtime": "^7.5.5",
    "@hapi/joi": "^17.1.0",
    "@sendgrid/mail": "^6.5.1",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.6",
    "babel-node": "0.0.1-security",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "bcrypt": "^3.0.6",

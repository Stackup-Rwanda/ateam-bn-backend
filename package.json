{
  "name": "ateam-bn-backend",
  "version": "1.0.0",
  "description": "We make company global travel and accommodation easy and convenient for the strong workforce of savvy members of staff, by leveraging the modern web.",
  "main": "index.js",
  "scripts": {
    "test": "nyc --reporter=text --reporter=lcov mocha --require @babel/register --require babel-polyfill ./server/tests/*.js --timeout 50000  --exit",
    "createAndInsertIntoValidTokensTable": "babel-node ./server/source/models/migrations/insertValidTokenTableQuery.js",
    "connectDBWithSequelize": "babel-node ./server/source/configDB/dbConnectWithSequelize.js.js",
    "connectDBWithQuery": "babel-node ./server/source/configDB/dbConnectWithQuery.js.js",
    "server": "nodemon --exec babel-node ./server/index.js",
    "start": "cross-env NODE_ENV=production babel-node ./server/index.js",
    "createtables:dev": "cross-env NODE_ENV=dev node_modules/.bin/sequelize db:migrate",
    "seed:dev": "cross-env NODE_ENV=dev node_modules/.bin/sequelize db:seed:all",
    "droptables:dev": "cross-env NODE_ENV=dev node_modules/.bin/sequelize db:migrate:undo",
    "createtables:test": "cross-env NODE_ENV=test node_modules/.bin/sequelize db:migrate",
    "seed:test": "cross-env NODE_ENV=test node_modules/.bin/sequelize db:seed:all",
    "coveralls": "npm test && nyc report --reporter=text-lcov | coveralls",
    "droptables:test": "cross-env NODE_ENV=test node_modules/.bin/sequelize db:migrate:undo"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Stackup-Rwanda/ateam-bn-backend.git"
  },
  "author": "The A Team",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Stackup-Rwanda/ateam-bn-backend/issues"
  },
  "homepage": "https://github.com/Stackup-Rwanda/ateam-bn-backend#readme",
  "dependencies": {
    "dotenv": "^8.2.0",
    "pg": "^7.17.1",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.21.3",
    "@babel/node": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.5.5",
    "@babel/runtime": "^7.5.5",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.6",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "body-parser": "^1.19.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "coveralls": "^3.0.9",
    "express": "^4.17.1",
    "mocha": "^7.0.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^2.0.2",
    "nyc": "^15.0.0",
    "swagger-ui-express": "^4.1.1"
  },
  "devDependencies": {
    "babel-install": "2.1.0",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "prettier": "^1.19.1",
    "sequelize-cli": "^5.5.1"
  },
  "engines": {
    "node": "10.16.3",
    "npm": "6.9.0"
  }
}
